  <% @users.each do |user| %>
        <h3> Username: <a href="/users/<%= user["user_id"] %>"><%= user["username"] %></a></h3>

        <h4> Bio: <%= user["bio"] %></h4>
        <h4> Visits: <% user["visits"].each do |visit|  %></h4>
            <h5> <%= visit["city"]["name"] + ", " + visit["country"]["name"] %> </h5>
            <% end %>
  <% end %>